generator client {
	provider = "prisma-client-js"
}

datasource db {
	provider = "postgresql"
}

model PublishedImage {
	id        Int      @id @default(autoincrement())
	imageUrl  String   @map("image_url")
	prompt    String
	hearts    Int      @default(0)
	createdAt DateTime @default(now()) @map("created_at")

	@@map("published_images")
}

